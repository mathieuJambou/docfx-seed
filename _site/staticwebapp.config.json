{
  "routes": [
    {
      "route": "/articles/*",
      "allowedRoles": ["reader"]
    }
  ],
  "navigationFallback": {
    "rewrite": "index.html",
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/401"
    },
    "403": {
      "rewrite": "/403"
    },
    "404": {
      "rewrite": "/404"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: blob: 'unsafe-eval' 'unsafe-inline'; object-src 'self' blob:; img-src 'self' blob: data: *"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
